		<classes>
			<class name="FrameWorkSample.EndToEndTest.HomePage" />
		</classes>
	</test>
	<test name="Validating Login Page">
		<classes>
			<class name="FrameWorkSample.EndToEndTest.LogIn" />
		</classes>
	</test>
</suite> <!-- Suite -->
